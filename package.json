{
  "name": "create-tauri-ui",
  "version": "0.2.2",
  "description": "ðŸ¦€ Create modern Tauri desktop apps in just a few simple steps.",
  "main": "index.js",
  "author": "agmmnn",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "start": "unbuild --stub && node index.js",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "format": "npx prettier --write . --ignore-path .gitignore ",
    "taze": "taze minor -r -w -i false -I",
    "taze:major": "taze major -r -w -i false -I",
    "updatesub": "git submodule update --init --recursive --remote"
  },
  "bin": {
    "create-tauri-ui": "index.js"
  },
  "files": [
    "index.js",
    "templates",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agmmnn/tauri-ui.git"
  },
  "bugs": {
    "url": "https://github.com/agmmnn/tauri-ui/issues"
  },
  "homepage": "https://github.com/agmmnn/tauri-ui",
  "dependencies": {
    "@clack/core": "^0.3.2",
    "@clack/prompts": "^0.6.3",
    "picocolors": "^1.0.0",
    "sisteransi": "^1.0.5",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.2",
    "@types/node": "^20.4.5",
    "@types/prompts": "^2.4.4",
    "commander": "^11.0.0",
    "kolorist": "^1.8.0",
    "minimist": "^1.2.8",
    "prompts": "^2.4.2",
    "tauri-controls": "^0.0.6",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1"
  }
